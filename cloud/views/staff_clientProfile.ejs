   value: {__type: "Pointer", className: "Clients", objectId: ui.item.idx},                            itemid: "tablecontent"                        });                }          });            $("#btn_repack").click(function(){                var consolidate = ''; /* = "Consolidate detail:\n\n"; */                var checkboxes = document.getElementsByName("checkware");              var checkbox;              for(i in checkboxes)        {                        if(checkboxes[i].checked)            {                /*                alert(checkboxes[i].id);                AQUI HAY QUE CREAR EL STRING QUE SE PASARA AL FORM DE WAREHOUSE CON LA INFORMACION PERTINENTE                */                                consolidate += checkboxes[i].id + ',';            }        }        /*        window.open("/staff_warehouse_consolidate/" + consolidate, "Microsoft","height=707,width=878");        */        alert(consolidate);	        return false;    });                           </script>            </body>	</html>