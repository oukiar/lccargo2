( "#txt_name" ).autocomplete({source: []});   	$("#txt_name").keyup(		function(){			var myval = $("#txt_name").val();						if(myval.length >= 1){				$.post("/autofill_client", 				{"txt_search":myval}, 				function(data){					result = JSON.parse(data);                    newresult = [];                    for(var i=0; i<result.length; i++){                        newresult.push({label:result[i]["Name"], idx:result[i]["objectId"]});                    }										$( "#txt_name" ).autocomplete({source: newresult});   					$('#txt_name').trigger("keydown");				});			}		});                /*CLIENT SELECCION FROM THE RESULT*/        $( "#txt_name" ).autocomplete({             select: 			 function( event, ui ){                    InsertTable({                            cols:   /*Columnas definidas FIELD:TITLE or FIELD:{FIELDNEEDED:FIELDTITLE}*/                                 {                                    "@<input type='checkbox' name='checkware' id='%'>":"Check", /* DINAMIC REPLACE FOR TABLE */                                    "Receipt":"Rcpt. #",                                    "Shipper":{"Name":"Agency"}, /*This is the sintax for pointers showed on table*/                                    "ReceiptDate":"Date",                                    "Country":"Country",                                    "Volume":"Volume",                                    "Warehouse":"WareHouse",                                    "Invoice":"Invoice",                                    "Transport":"Transport"                                },                                                        class:"table table-bordered", /* CSS STYLE */                            collection: "WareHouse", /* COLLECCION PRINCIPAL */                            by: "Consignee",    /* CAMPO DE BUSQUEDA */                            value: {__type: "Pointer", className: "Clients", objectId: ui.item.idx},                            itemid: "tablecontent"                        });                }          });            $("#btn_repack").click(function(){                var consolidate = ''; /* = "Consolidate detail:\n\n"; */                var checkboxes = document.getElementsByName("checkware");              var checkbox;              for(i in checkboxes)        {                        if(checkboxes[i].checked)            {                /*                alert(checkboxes[i].id);                AQUI HAY QUE CREAR EL STRING QUE SE PASARA AL FORM DE WAREHOUSE CON LA INFORMACION PERTINENTE                */                                consolidate += checkboxes[i].id + ',';            }        }        /*        window.open("/staff_warehouse_consolidate/" + consolidate, "Microsoft","height=707,width=878");        */        alert(consolidate);	        return false;    });                           </script>            </body>	</html>