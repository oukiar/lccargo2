<div id="clientstable">      <ul id="list">        <table class="table table-bordered" id="blueRow">        <tr>              <th>Name</th>              <th>Company</th>              <th>Country</th>            <th>Address</th>            <th>Tel</th>            <th>Email</th>            <th>Destination</th>        </tr>        <% clients.forEach(function(client){%>                    <tr dir-paginate="product in product | filter:q |" current-page="currentPage">                <td><%= client.get("Name")%></td>                <td><%= client.get("Company")%></td>                <td><%= client.get("Country")%></td>                <td><%= client.get("Address")%></td>                                <td><%= client.get("Telephone")%></td>                <td><%= client.get("Email")%></td>                <td><%= client.get("Destination")%></td>                                </tr>                     <% }) %>        </table>      </ul></div>