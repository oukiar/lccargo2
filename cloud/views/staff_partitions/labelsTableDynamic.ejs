<div id="ware_table">    <table class="table table-bordered" id="blueRow">    <tr>            <th>Rcpt. #</th>            <th>Client</th>            <th>Shipper</th>            <th>Invoice</th>            <th>Date</th>            <th>Country</th>            <th>Transport</th>            <th>Volume</th>            <th>WareHouse</th>    </tr>    <% results.forEach(function(label){%>        <tr dir-paginate="product in product | filter:q |" current-page="currentPage">                    <td align="center"><%= label["Receipt"]%></td>            <td><%= label["Consignee"]%></td>            <td><%= label["Shipper"]%></td>            <td><%= label["Invoice"]%></td>            <td><%= label["ReceiptDate"]%></td>            <td><%= label["Country"]%></td>            <td><%= label["Transport"]%></td>            <td><%= label["Volume"]%></td>            <td><%= label["Warehouse"]%></td>        </tr>    <% }) %>       </table></div>