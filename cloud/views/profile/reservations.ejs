<div id="tabs-4"><div class="pull-right"></div><div class="row">    <div class="col-lg-12 col-md-12 container">        <div class="row">            <div class="col-md-6 col-xs-6" style="margin-top:13px;"></div>                <div class="col-md-6 col-xs-6" style="margin-top:10px;">                    <form class="pure-form" method="post" action="/staff_warehouse_filter" align="right">                        <label for="filterby" class="pfont">By:</label>                                           <select  id="filterby" name="filterby">                            <option value="All">All</option>                            <option value="Airline">Consignee</option>                            <option value="Arrivals">Shipper</option>                            <option value="Departure">Receipt #</option>                            <option value="Destination">Date</option>                            <option value="Flight">Transport</option>                            <option value="People">Invoice</option>                            <option value="Status">Country</option>                            <option value="Notes">WareHouse</option>                        </select>                        <!-- IF ALL OR DATE SELECTED THIS SEARCH INPUT IS NOT USE-->                        <input type="hidden" placeholder="Search"  id="searchtext" name="searchtext">                                   <label class="pfont">Date:</label>                        <select id="filterdate" name="filterdate">                            <option value="All">All</option>                            <option value="Today">Today</option>                            <option value="Yesterday">Yesterday</option>                            <option value="This week">This week</option>                            <option value="This month">This month</option>                            <option value="Last month">Last month</option>	                            <option value="Current year">Current Year</option>                        </select>			                                                   <button type="submit" class="pure-button pure-button-primary">Search</button>                    </form>                </div>            </div>                  <div class="panel">                <div class="panel-body">                    <ul id="list">                        <table class="table table-bordered" id="blueRow">                            <tr>                                <th>Airline</th>                                <th>Arrivals</th>                                <th>Departure</th>                                <th>Destination</th>                                <th>Flight</th>                                <th>People</th>                                <th>Status</th>                                <th>Notes</th>                            </tr>                            <% results.forEach(function(label){%>                                <tr>                                    <td align="center"><%= label["Airline"]%></td>                                    <td><%= label["Arrivals"]%></td>                                    <td><%= label["Departure"]%></td>                                    <td><%= label["Destination"]%></td>                                    <td><%= label["Flight"]%></td>                                    <td><%= label["People"]%></td>                                    <td><%= label["Status"]%></td>                                    <td><%= label["Notes"]%></td>                                </tr>                            <% }) %>                                </table>                    </ul>                </div>            </div>        </div>    </div> </div>